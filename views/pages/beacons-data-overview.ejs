<div id="beacons-data-overview" v-cloak>

  <h1>Graphs.</h1>
  
  <div>
  <button type="button" class="btn btn-outline-primary"
   data-toggle="collapse" data-target="#collapseTreemapSquare" aria-expanded="false"
    aria-controls="collapseTreemapSquare"><i class="fa" />Treemap</button></div>
  <br /><br />
  <div class="collapse multi-collapse" id="collapseTreemapSquare">
    <div class="card card-body">
      <svg id="svgTreemapSquare" width="960" height="570"></svg>
    </div>
  </div>


  <div>
    <button type="button" class="btn btn-outline-primary"
     data-toggle="collapse" data-target="#collapsCirclePacking" aria-expanded="false"
      aria-controls="collapsCirclePacking"><i class="fa" />Circular Treemap</button></div>
    <br /><br />
    <div class="collapse multi-collapse" id="collapsCirclePacking">
      <div class="card card-body">
        <svg id="svgCirclePacking" width="960" height="960"></svg>
      </div>
    </div>

    <div>
      <button type="button" class="btn btn-outline-primary"
       data-toggle="collapse" data-target="#collapseGraphForces" aria-expanded="false"
        aria-controls="collapseGraphForces"><i class="fa" />Graph Forces directed</button></div>
      <br /><br />
      <div class="collapse multi-collapse" id="collapseGraphForces">
        <div class="card card-body">
          <svg id="svgGraphForces" width="960" height="960"></svg>
        </div>
      </div>

</div>
